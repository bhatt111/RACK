        <j.1:createdBy rdf:resource="#LlrDev1"/>
        <j.1:governs rdf:resource="#ExecutiveThread"/>
        <j.1:text>Executive shall spawn Input Thread on powerup.</j.1:text>
        <j.2:identifier>EXE-LLR-1</j.2:identifier>
      </j.0:LowLevelRequirement>
    </j.1:consumedBy>
