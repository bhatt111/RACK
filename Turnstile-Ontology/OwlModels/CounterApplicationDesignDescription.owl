        <j.1:governs rdf:resource="#ExecutiveThread"/>
        <j.1:text>Executive shall spawn Output Thread on powerup.</j.1:text>
        <j.2:identifier>EXE-LLR-2</j.2:identifier>
      </j.0:LowLevelRequirement>
    </j.1:consumedBy>
    <j.1:consumedBy>
