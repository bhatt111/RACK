    <j.1:text>incoming UDP message</j.1:text>
    <j.2:identifier>DCC-2</j.2:identifier>
  </j.0:DataAndControlCouple>
  <j.0:DataAndControlCouple rdf:ID="DCC-1">
    <j.1:createdBy rdf:resource="#LlrDev1"/>
