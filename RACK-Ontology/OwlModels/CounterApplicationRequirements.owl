<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:j.0="http://arcos.rack/PROV-S#"
    xmlns:cntrapprq="http://TurnstileSystem/CounterApplicationRequirements#"
    xmlns:ha="http://TurnstileSystem/HazardAssessment#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:sadlimplicitmodel="http://sadl.org/sadlimplicitmodel#"
    xmlns:trnstl="http://TurnstileSystem/Turnstiles#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:sadlbasemodel="http://sadl.org/sadlbasemodel#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:Rq="http://arcos.rack/REQUIREMENTS#"
  xml:base="http://TurnstileSystem/CounterApplicationRequirements">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="HazardAssessment"/>
    <owl:imports rdf:resource="Turnstiles"/>
    <owl:imports rdf:resource="http://arcos.rack/REQUIREMENTS"/>
    <owl:imports rdf:resource="http://sadl.org/sadlimplicitmodel"/>
    <owl:imports rdf:resource="http://sadl.org/sadlbasemodel"/>
    <rdfs:comment xml:lang="en">This ontology was created from a SADL file 'CounterApplicationRequirements.sadl' and should not be directly edited.</rdfs:comment>
  </owl:Ontology>
  <Rq:DATA_DICTIONARY_TERM rdf:ID="inflowEvent">
    <Rq:consumedBy>
      <Rq:REQUIREMENT rdf:ID="HLR-1">
        <Rq:governs rdf:resource="Turnstiles#CounterApplication"/>
        <Rq:mitigates rdf:resource="HazardAssessment#H-1.2"/>
        <Rq:satisfies rdf:resource="Turnstiles#Sys-1"/>
        <Rq:text>The Computer shall increment the counter when a inflow event is received and the counter is less than max int.</Rq:text>
        <j.0:identifier>HLR-1</j.0:identifier>
      </Rq:REQUIREMENT>
    </Rq:consumedBy>
    <Rq:providedBy rdf:resource="Turnstiles#inflow"/>
    <j.0:identifier>inflowEvent</j.0:identifier>
  </Rq:DATA_DICTIONARY_TERM>
  <Rq:DATA_DICTIONARY_TERM rdf:ID="display">
    <Rq:consumedBy rdf:resource="Turnstiles#census"/>
    <Rq:providedBy>
      <Rq:REQUIREMENT rdf:ID="HLR-3">
        <Rq:governs rdf:resource="Turnstiles#CounterApplication"/>
        <Rq:satisfies rdf:resource="Turnstiles#Sys-3"/>
        <Rq:text>The Computer shall publish the counter at a 1 htz rate.</Rq:text>
        <j.0:identifier>HLR-3</j.0:identifier>
      </Rq:REQUIREMENT>
    </Rq:providedBy>
    <j.0:identifier>display</j.0:identifier>
  </Rq:DATA_DICTIONARY_TERM>
  <Rq:DATA_DICTIONARY_TERM rdf:ID="outflowEvent">
    <Rq:consumedBy>
      <Rq:REQUIREMENT rdf:ID="HLR-2">
        <Rq:governs rdf:resource="Turnstiles#CounterApplication"/>
        <Rq:mitigates rdf:resource="HazardAssessment#H-1.1"/>
        <Rq:satisfies rdf:resource="Turnstiles#Sys-2"/>
        <Rq:text>The Computer shall decrement the counter when a outflow event is received and the counter is greater than 0.</Rq:text>
        <j.0:identifier>HLR-2</j.0:identifier>
      </Rq:REQUIREMENT>
    </Rq:consumedBy>
    <Rq:providedBy rdf:resource="Turnstiles#outflow"/>
    <j.0:identifier>outflowEvent</j.0:identifier>
  </Rq:DATA_DICTIONARY_TERM>
  <Rq:DATA_DICTIONARY_TERM rdf:ID="counter">
    <Rq:consumedBy rdf:resource="#HLR-3"/>
    <Rq:providedBy rdf:resource="#HLR-2"/>
    <Rq:providedBy rdf:resource="#HLR-1"/>
    <j.0:identifier>counter</j.0:identifier>
  </Rq:DATA_DICTIONARY_TERM>
</rdf:RDF>
