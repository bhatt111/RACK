<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:j.0="http://arcos.rack/PROV-S#"
    xmlns:sys="http://arcos.rack/SYSTEM#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:sadlimplicitmodel="http://sadl.org/sadlimplicitmodel#"
    xmlns:trnstl="http://TurnstileSystem/Turnstiles#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:sadlbasemodel="http://sadl.org/sadlbasemodel#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:Rq="http://arcos.rack/REQUIREMENTS#"
  xml:base="http://TurnstileSystem/Turnstiles">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://arcos.rack/REQUIREMENTS"/>
    <owl:imports rdf:resource="http://arcos.rack/SYSTEM"/>
    <owl:imports rdf:resource="http://sadl.org/sadlimplicitmodel"/>
    <owl:imports rdf:resource="http://sadl.org/sadlbasemodel"/>
    <rdfs:comment xml:lang="en">This ontology was created from a SADL file 'Turnstiles.sadl' and should not be directly edited.</rdfs:comment>
  </owl:Ontology>
  <Rq:REQUIREMENT rdf:ID="Sys-3">
    <Rq:governs>
      <sys:SYSTEM rdf:ID="Turnstiles">
        <j.0:identifier>TurnStileSystem</j.0:identifier>
      </sys:SYSTEM>
    </Rq:governs>
    <Rq:text>Turnstile system shall track the number of people are currently in the park.</Rq:text>
    <j.0:identifier>Sys-3</j.0:identifier>
  </Rq:REQUIREMENT>
  <sys:INTERFACE rdf:ID="outflow">
    <sys:destination>
      <sys:SYSTEM rdf:ID="CounterApplication">
        <sys:partOf rdf:resource="#Turnstiles"/>
        <j.0:identifier>Counter Application</j.0:identifier>
      </sys:SYSTEM>
    </sys:destination>
    <sys:source>
      <sys:SYSTEM rdf:ID="Outgate">
        <sys:partOf rdf:resource="#Turnstiles"/>
        <j.0:identifier>Out Gate</j.0:identifier>
      </sys:SYSTEM>
    </sys:source>
    <j.0:identifier>outflow</j.0:identifier>
  </sys:INTERFACE>
  <sys:INTERFACE rdf:ID="inflow">
    <sys:destination rdf:resource="#CounterApplication"/>
    <sys:source>
      <sys:SYSTEM rdf:ID="Ingate">
        <sys:partOf rdf:resource="#Turnstiles"/>
        <j.0:identifier>In Gate</j.0:identifier>
      </sys:SYSTEM>
    </sys:source>
    <j.0:identifier>inflow</j.0:identifier>
  </sys:INTERFACE>
  <Rq:REQUIREMENT rdf:ID="Sys-1">
    <Rq:governs rdf:resource="#Turnstiles"/>
    <Rq:text>Turnstile system shall track the number of people that travel through the in gate.</Rq:text>
    <j.0:identifier>Sys-1</j.0:identifier>
  </Rq:REQUIREMENT>
  <Rq:REQUIREMENT rdf:ID="Sys-2">
    <Rq:governs rdf:resource="#Turnstiles"/>
    <Rq:text>Turnstile system shall track the number of people that travel through the out gate.	</Rq:text>
    <j.0:identifier>Sys-2</j.0:identifier>
  </Rq:REQUIREMENT>
  <sys:INTERFACE rdf:ID="census">
    <sys:destination rdf:resource="#Ingate"/>
    <sys:destination>
      <sys:SYSTEM rdf:ID="Display">
        <sys:partOf rdf:resource="#Turnstiles"/>
        <j.0:identifier>Display</j.0:identifier>
      </sys:SYSTEM>
    </sys:destination>
    <sys:source rdf:resource="#CounterApplication"/>
    <j.0:identifier>census</j.0:identifier>
  </sys:INTERFACE>
</rdf:RDF>
