<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:j.0="http://arcos.rack/PROV-S#"
    xmlns:ha="http://TurnstileSystem/HazardAssessment#"
    xmlns:builtinfunctions="http://sadl.org/builtinfunctions#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:ingaterq="http://TurnstileSystem/InGateRequirement#"
    xmlns:sadlimplicitmodel="http://sadl.org/sadlimplicitmodel#"
    xmlns:trnstl="http://TurnstileSystem/Turnstiles#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:sadlbasemodel="http://sadl.org/sadlbasemodel#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:Rq="http://arcos.rack/REQUIREMENTS#"
  xml:base="http://TurnstileSystem/InGateRequirement">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="HazardAssessment"/>
    <owl:imports rdf:resource="Turnstiles"/>
    <owl:imports rdf:resource="http://arcos.rack/REQUIREMENTS"/>
    <owl:imports rdf:resource="http://sadl.org/builtinfunctions"/>
    <owl:imports rdf:resource="http://sadl.org/sadlimplicitmodel"/>
    <owl:imports rdf:resource="http://sadl.org/sadlbasemodel"/>
    <rdfs:comment xml:lang="en">This ontology was created from a SADL file 'InGateRequirements.sadl' and should not be directly edited.</rdfs:comment>
  </owl:Ontology>
  <Rq:REQUIREMENT rdf:ID="IN-GATE-3">
    <Rq:governs rdf:resource="Turnstiles#Ingate"/>
    <Rq:satisfies rdf:resource="Turnstiles#Sys-1"/>
    <Rq:text>The Gate shall report when a person is admitted.</Rq:text>
    <j.0:identifier>IN-GATE-3</j.0:identifier>
  </Rq:REQUIREMENT>
  <Rq:REQUIREMENT rdf:ID="IN-GATE-2">
    <Rq:governs rdf:resource="Turnstiles#Ingate"/>
    <Rq:mitigates rdf:resource="HazardAssessment#H-2"/>
    <Rq:satisfies rdf:resource="Turnstiles#Sys-1"/>
    <Rq:text>The Gate shall be disabled when the maximum census is reached.</Rq:text>
    <j.0:identifier>IN-GATE-2</j.0:identifier>
  </Rq:REQUIREMENT>
  <Rq:REQUIREMENT rdf:ID="IN-GATE-1">
    <Rq:governs rdf:resource="Turnstiles#Ingate"/>
    <Rq:satisfies rdf:resource="Turnstiles#Sys-1"/>
    <Rq:text>The Gate shall permit only one person at a time.</Rq:text>
    <j.0:identifier>IN-GATE-1</j.0:identifier>
  </Rq:REQUIREMENT>
</rdf:RDF>
