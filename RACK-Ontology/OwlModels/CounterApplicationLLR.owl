<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:j.0="http://arcos.rack/PROV-S#"
    xmlns:cntrapprq="http://TurnstileSystem/CounterApplicationRequirements#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:cntrappllr="http://TurnstileSystem/CounterApplicationLLR#"
    xmlns:trnstlsys="http://TurnstileSystem/CounterApplication#"
    xmlns:sadlimplicitmodel="http://sadl.org/sadlimplicitmodel#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:sadlbasemodel="http://sadl.org/sadlbasemodel#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:Rq="http://arcos.rack/REQUIREMENTS#"
  xml:base="http://TurnstileSystem/CounterApplicationLLR">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="CounterApplicationRequirements"/>
    <owl:imports rdf:resource="CounterApplication"/>
    <owl:imports rdf:resource="http://arcos.rack/REQUIREMENTS"/>
    <owl:imports rdf:resource="http://sadl.org/sadlimplicitmodel"/>
    <owl:imports rdf:resource="http://sadl.org/sadlbasemodel"/>
    <rdfs:comment xml:lang="en">This ontology was created from a SADL file 'CounterApplicationLLR.sadl' and should not be directly edited.</rdfs:comment>
  </owl:Ontology>
  <Rq:DATA_DICTIONARY_TERM rdf:ID="DCC-6">
    <Rq:providedBy>
      <Rq:REQUIREMENT rdf:ID="IN-LLR-6">
        <Rq:governs rdf:resource="CounterApplication#InputThread"/>
        <Rq:text>Input Thread shall print 'Limit Exceeded'' to the console when a valid UDP message is received and the park_count range is exceed.</Rq:text>
        <j.0:identifier>IN-LLR-6</j.0:identifier>
      </Rq:REQUIREMENT>
    </Rq:providedBy>
    <Rq:providedBy>
      <Rq:REQUIREMENT rdf:ID="IN-LLR-5">
        <Rq:governs rdf:resource="CounterApplication#InputThread"/>
        <Rq:text>Input Thread shall print 'Invalid Message' to the console when a invalid UDP message is received.</Rq:text>
        <j.0:identifier>IN-LLR-5</j.0:identifier>
      </Rq:REQUIREMENT>
    </Rq:providedBy>
    <Rq:providedBy>
      <Rq:REQUIREMENT rdf:ID="EXE-LLR-3">
        <Rq:governs rdf:resource="CounterApplication#ExecutiveThread"/>
        <Rq:text>Executive shall print a single '.' character to the console every second when running.</Rq:text>
        <j.0:identifier>EXE-LLR-3</j.0:identifier>
      </Rq:REQUIREMENT>
    </Rq:providedBy>
    <Rq:text>console</Rq:text>
    <j.0:identifier>DCC-6</j.0:identifier>
  </Rq:DATA_DICTIONARY_TERM>
  <Rq:DATA_DICTIONARY_TERM rdf:ID="DCC-5">
    <Rq:providedBy>
      <Rq:REQUIREMENT rdf:ID="OUT-LLR-2">
        <Rq:governs rdf:resource="CounterApplication#OutputThread"/>
        <Rq:satisfies rdf:resource="CounterApplicationRequirements#HLR-3"/>
        <Rq:text>Output Thread shall broadcast a UDP message on port 62001 with the park_count every second.</Rq:text>
        <j.0:identifier>OUT-LLR-2</j.0:identifier>
      </Rq:REQUIREMENT>
    </Rq:providedBy>
    <Rq:text>outgoing UDP message</Rq:text>
    <j.0:identifier>DCC-5</j.0:identifier>
  </Rq:DATA_DICTIONARY_TERM>
  <Rq:DATA_DICTIONARY_TERM rdf:ID="DCC-4">
    <Rq:providedBy>
      <Rq:REQUIREMENT rdf:ID="IN-LLR-3">
        <Rq:governs rdf:resource="CounterApplication#InputThread"/>
        <Rq:satisfies rdf:resource="CounterApplicationRequirements#HLR-2"/>
        <Rq:satisfies rdf:resource="CounterApplicationRequirements#HLR-1"/>
        <Rq:text>Input Thread shall add the delta value received by the UDP to the park_count and send the updated park_count to the Output Thread when a valid UDP message is received and the park_count range is not exceed.</Rq:text>
        <j.0:identifier>IN-LLR-3</j.0:identifier>
      </Rq:REQUIREMENT>
    </Rq:providedBy>
    <Rq:providedBy>
      <Rq:REQUIREMENT rdf:ID="OUT-LLR-1">
        <Rq:governs rdf:resource="CounterApplication#OutputThread"/>
        <Rq:text>Output Thread shall initialize the park_count to 0 on powerup.</Rq:text>
        <j.0:identifier>OUT-LLR-1</j.0:identifier>
      </Rq:REQUIREMENT>
    </Rq:providedBy>
    <Rq:consumedBy rdf:resource="#OUT-LLR-2"/>
    <Rq:text>output_park_count</Rq:text>
    <j.0:identifier>DCC-4</j.0:identifier>
  </Rq:DATA_DICTIONARY_TERM>
  <Rq:DATA_DICTIONARY_TERM rdf:ID="DCC-3">
    <Rq:providedBy>
      <Rq:REQUIREMENT rdf:ID="IN-LLR-1">
        <Rq:governs rdf:resource="CounterApplication#InputThread"/>
        <Rq:text>Input Thread shall initialize the park_count to 0 on powerup.</Rq:text>
        <j.0:identifier>IN-LLR-1</j.0:identifier>
      </Rq:REQUIREMENT>
    </Rq:providedBy>
    <Rq:consumedBy rdf:resource="#IN-LLR-3"/>
    <Rq:consumedBy rdf:resource="#IN-LLR-5"/>
    <j.0:identifier>DCC-3</j.0:identifier>
    <Rq:consumedBy>
      <Rq:REQUIREMENT rdf:ID="IN-LLR-2">
        <Rq:governs rdf:resource="CounterApplication#InputThread"/>
        <Rq:satisfies rdf:resource="CounterApplicationRequirements#HLR-2"/>
        <Rq:satisfies rdf:resource="CounterApplicationRequirements#HLR-1"/>
        <Rq:text>Input Thread shall check for a incoming UDP message on port 62000.</Rq:text>
        <j.0:identifier>IN-LLR-2</j.0:identifier>
      </Rq:REQUIREMENT>
    </Rq:consumedBy>
    <Rq:text>input_park_count</Rq:text>
    <Rq:consumedBy>
      <Rq:REQUIREMENT rdf:ID="IN-LLR-4">
        <Rq:governs rdf:resource="CounterApplication#InputThread"/>
        <Rq:text>Input Thread shall limit park_count to between 0 and 1500.</Rq:text>
        <j.0:identifier>IN-LLR-4</j.0:identifier>
      </Rq:REQUIREMENT>
    </Rq:consumedBy>
    <Rq:consumedBy rdf:resource="#IN-LLR-6"/>
    <Rq:providedBy rdf:resource="#IN-LLR-4"/>
  </Rq:DATA_DICTIONARY_TERM>
  <Rq:DATA_DICTIONARY_TERM rdf:ID="DCC-2">
    <Rq:consumedBy rdf:resource="#IN-LLR-6"/>
    <Rq:consumedBy rdf:resource="#IN-LLR-5"/>
    <Rq:consumedBy rdf:resource="#IN-LLR-3"/>
    <Rq:consumedBy rdf:resource="#IN-LLR-2"/>
    <Rq:text>incoming UDP message</Rq:text>
    <j.0:identifier>DCC-2</j.0:identifier>
  </Rq:DATA_DICTIONARY_TERM>
  <Rq:DATA_DICTIONARY_TERM rdf:ID="DCC-1">
    <Rq:consumedBy rdf:resource="#OUT-LLR-1"/>
    <Rq:consumedBy rdf:resource="#IN-LLR-1"/>
    <Rq:consumedBy>
      <Rq:REQUIREMENT rdf:ID="EXE-LLR-2">
        <Rq:governs rdf:resource="CounterApplication#ExecutiveThread"/>
        <Rq:text>Executive shall spawn Output Thread on powerup.</Rq:text>
        <j.0:identifier>EXE-LLR-2</j.0:identifier>
      </Rq:REQUIREMENT>
    </Rq:consumedBy>
    <Rq:consumedBy>
      <Rq:REQUIREMENT rdf:ID="EXE-LLR-1">
        <Rq:governs rdf:resource="CounterApplication#ExecutiveThread"/>
        <Rq:text>Executive shall spawn Input Thread on powerup.</Rq:text>
        <j.0:identifier>EXE-LLR-1</j.0:identifier>
      </Rq:REQUIREMENT>
    </Rq:consumedBy>
    <Rq:text>PowerUp</Rq:text>
    <j.0:identifier>DCC-1</j.0:identifier>
  </Rq:DATA_DICTIONARY_TERM>
</rdf:RDF>
